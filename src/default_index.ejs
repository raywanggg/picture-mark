<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
  </head>
  <body>
  	<canvas id="drawing" width="1150" height="650"></canvas>
  	<!-- <img id="cartoon" src="<%= htmlWebpackPlugin.options.imgUrl %>" alt="cartoon"> -->

  	<!-- 标记栏 -->
	<div id="control-panel">
		<a class="pin-click" data-shape-type="dot" href="javascript:void(0)" style="top:0;" >
			<div class="btn-pin" data-shape-type="dot"></div>
		</a>
		<a class="rect-click" data-shape-type="rect" href="javascript:void(0)" style="top:80px;">
			<div class="btn-rect" data-shape-type="rect"></div>
		</a>
		<a class="ellipse-click" data-shape-type="circle" href="javascript:void(0)" style="top:160px;">
			<div class="btn-ellipse" data-shape-type="circle"></div>
		</a>
		<a class="line-click" data-shape-type="arrow" href="javascript:void(0)" style="top:240px;">
			<div class="btn-line" data-shape-type="arrow"></div>
		</a>
		<a class="save-click" data-shape-type="save" href="javascript:void(0)" style="top:320px;">save</a>
	</div>

	<!-- 滤镜栏 -->
	<ul id="filter-panel">
		<a class="filter" data-filter-type="Ori" href="javascript:void(0)" style="background: transparent; border: 1px solid #dedede">
			<div data-filter-type="Ori">Ori</div>
		</a>
		<a class="filter" data-filter-type="GRAY" href="javascript:void(0)" style="background: #dedede">
			<div data-filter-type="GRAY"></div>
		</a>
		<a class="filter" data-filter-type="DARK" href="javascript:void(0)" style="background: #666">
			<div data-filter-type="DARK"></div>
		</a>
		<a class="filter" data-filter-type="BROWN" href="javascript:void(0)" style="background: #A52A2A">
			<div data-filter-type="BROWN"></div>
		</a>
		<a class="filter" data-filter-type="RED" href="javascript:void(0)" style="background: #FF0000">
			<div data-filter-type="RED"></div>
		</a>
		<a class="filter" data-filter-type="GREEN" href="javascript:void(0)" style="background: #228B22">
			<div data-filter-type="GREEN"></div>
		</a>
		<a class="filter" data-filter-type="BLUE" href="javascript:void(0)" style="background: #6495ED">
			<div data-filter-type="BLUE"></div>
		</a>
		<a class="filter" data-filter-type="BLACK_WHITE" href="javascript:void(0)" style="background: transparent; border: 1px solid #dedede">
			<div data-filter-type="BLACK_WHITE">B_W</div>
		</a>
		<a class="filter" data-filter-type="REVERSE" href="javascript:void(0)" style="background: transparent; border: 1px solid #dedede; width: 60px">
			<div data-filter-type="REVERSE">REVERSE</div>
		</a>
		<a class="filter" data-filter-type="RELIEF" href="javascript:void(0)" style="background: transparent; border: 1px solid #dedede; width: 60px">
			<div data-filter-type="RELIEF">RELIEF</div>
		</a>
	</ul>
  </body>
</html>